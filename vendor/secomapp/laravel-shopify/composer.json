{
    "name": "secomapp/laravel-shopify",
    "description": "Laravel Shopify App Package",
    "keywords": ["laravel", "shopify", "app"],
    "license": "MIT",
    "require": {
        "php" : "^7.1.3",
        "illuminate/database": "~5.6",
        "illuminate/support": "~5.6",
        "illuminate/console": "~5.6",
        "nesbot/carbon": "~1.0",
        "symfony/http-kernel": "~3.0|~4.0",
        "meenie/javascript-packer": "~1.1",
        "spatie/laravel-activitylog": "^2.7.0",
        "secomapp/shopify-api": "^1.0",
        "cadicvnn/shopify-php": "dev-master",
        "tubalmartin/cssmin": "^4.1",
        "voku/html-min": "^3.0"
    },
    "require-dev": {
        "illuminate/routing": "~5.6",
        "illuminate/http": "~5.6",
        "illuminate/view": "~5.6",
        "fzaninotto/faker": "~1.4",
        "orchestra/testbench": "~3.6.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "vlucas/phpdotenv": "~2.5.0"
    },
    "autoload": {
        "psr-4": {
            "Secomapp\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Secomapp\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "cadicvnn/shopify-php": "dev-master",
            "*" : "dist"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
